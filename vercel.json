{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api/users",
      "dest": "routes/api/auth.js"
    },
     {
      "src": "/api/contacts",
      "dest": "routes/api/contacts.js"
    },
         {
      "src": "/users",
      "dest": "/routes/users.js"
    },
             {
      "src": "/contacts",
      "dest": "/routes/contacts.js"
    }
  ]
}